{
  "name": "blog",
  "version": "0.0.0",
  "devDpendencies": {
    "less": "^1.7.4",
    "less-watchr": "^1.2.0"
  },
  "scripts": {
    "serve": "jekyll serve --watch",
    "css": "node node_modules/less/bin/lessc assets/less/style.less > assets/css/style.css",
    "watch": "node node_modules/less-watchr/bin/less-watchr --path assets/less --fileToCompile assets/less/style.less --outputFilename ../css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/julesbou/julesbou.github.io.git"
  }
}
